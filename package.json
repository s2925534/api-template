{
  "name": "api-template",
  "version": "1.0.0",
  "description": "API template to quickly spin a new API codebase",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Express",
    "Sequelize",
    "Node.js",
    "REST",
    "MySQL",
    "Backend",
    "Database",
    "Template",
    "ORM",
    "Web API",
    "Server",
    "CRUD",
    "Scalable"
  ],
  "author": "veloso <pedro@veloso.dev>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "chatgpt": "^5.2.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsdom": "^25.0.1",
    "mysql2": "^3.11.4",
    "node-fetch": "^2.7.0",
    "pdf-parse": "^1.1.1",
    "pdf2html": "^3.1.0",
    "sequelize": "^6.37.5",
    "velosify-ai": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.6",
    "@types/pdf-parse": "^1.1.4",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
